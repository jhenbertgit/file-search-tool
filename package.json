{
  "name": "file-search",
  "productName": "File Search",
  "version": "1.0.0-beta.3",
  "description": "A modern file search application",
  "main": "dist/infrastructure/electron/main-process.js",
  "$schema": "http://json.schemastore.org/package",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "package:win": "electron-forge package --platform win32",
    "make:win": "electron-forge make --platform win32",
    "publish:win": "electron-forge publish --platform win32",
    "build": "tsc && node copy-files.js",
    "lint": "eslint src/**/*.js"
  },
  "keywords": [
    "file",
    "search",
    "electron"
  ],
  "author": {
    "name": "Jhenbert Villamucho",
    "email": "jvv205756@gmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "@types/jest": "^30.0.0",
    "chokidar": "^3.6.0",
    "dotenv": "^17.2.1",
    "electron-is": "^3.0.0",
    "electron-squirrel-startup": "^1.0.1",
    "fs-extra": "^11.2.0",
    "inversify": "^7.9.0",
    "jest": "^30.0.5",
    "reflect-metadata": "^0.2.2",
    "ts-jest": "^29.4.1"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.8.3",
    "@electron-forge/maker-deb": "^7.8.3",
    "@electron-forge/maker-rpm": "^7.8.3",
